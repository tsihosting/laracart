{
  "name": "tsihosting/laracart",
  "description": "A Shopping Cart system built for Laravel.",
  "keywords": [
    "laravel shopping cart"
  ],
  "authors": [
    {
      "name": "Tim Igoe",
      "email": "tim@tsihosting.co.uk"
    }
  ],
  "require": {
    "php":">=7.2"
  },
  "require-dev": {
    "illuminate/session": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "TsiHosting\\LaraCart\\": "src/"
    }
  },
  "license": "MIT",
  "extra": {
    "laravel": {
      "providers": [
        "TsiHosting\\LaraCart\\ServiceProvider"
      ],
      "aliases": {
        "Cart": "TsiHosting\\LaraCart\\Facade"
      }
    }
  }
}